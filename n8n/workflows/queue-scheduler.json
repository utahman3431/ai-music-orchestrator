{
  "name": "Queue - Scheduler",
  "nodes": [
    {
      "parameters": {
        "triggerTimes": [
          {
            "mode": "everyMinute"
          }
        ]
      },
      "name": "Cron - Every Minute",
      "type": "n8n-nodes-base.cron",
      "typeVersion": 1,
      "position": [
        -200,
        300
      ]
    },
    {
      "parameters": {
        "url": "={{ ($env.N8N_WEBHOOK_BASE_URL || $env.N8N_API_URL || 'http://127.0.0.1:5678').replace(/\\/+$/, '') + '/webhook/worker/run' }}",
        "method": "POST",
        "allowUnauthorizedCerts": true,
        "sendBody": true,
        "options": {},
        "queryParametersUi": {
          "parameters": [
            {
              "name": "token",
              "value": "={{$env.WORKER_TOKEN || $env.N8N_WORKER_TOKEN || ''}}"
            }
          ]
        }
      },
      "name": "HTTP - Trigger Worker",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 1,
      "position": [
        200,
        300
      ]
    }
  ],
  "connections": {
    "Cron - Every Minute": {
      "main": [
        [
          {
            "node": "HTTP - Trigger Worker",
            "type": "main",
            "index": 0
          }
        ]
      ]
    }
  },
  "active": false,
  "settings": {
    "executionOrder": "v1"
  },
  "tags": [
    {
      "name": "ai-music"
    }
  ]
}
